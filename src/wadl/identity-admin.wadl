    <!-- Manage user's API key -->
    
    <method name="GET" id="admin-getUserCredential">
        <wadl:doc xmlns="http://docbook.org/ns/docbook" xml:lang="EN" title="Get user API key credentials">
            <para role="shortdesc">Get user credentials.</para>
            <para>To list API key credentials for a specified user, include the user ID in the request. If you don't know the ID, 
                use the <link xlink:href="GET_admin-listUsers_v2.0_users_User_Calls.html">List users</link> operation to find it.
            </para>
        </wadl:doc>
        <response status="200 203">
