                        <xsdxt:code
                            href="../common/samples/version.json"/>
                    </xsdxt:sample>
                </wadl:doc>
            </representation>
            
            <representation mediaType="application/xml" element="version2">
                <wadl:doc xmlns="http://docbook.org/ns/docbook">
                    <xsdxt:sample
